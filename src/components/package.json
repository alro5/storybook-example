    "ts-jest": "^27.1.3",
    "jest-cli": "^27.5.1",
    "prettier": "^2.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
